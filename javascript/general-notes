// example of looping function for Reduce Method
// ages of each student
let ages = [13, 1, 43, 1];


// provided function
let sum = function(previousValue, currentValue) {
	return previousValue + currentValue;
};

// for loop

/*
for (let i = 0; i < ages.length; i++) {
	sum(ages[i]);
};
*/
let total = ages.reduce(sum, 10);
console.log(total);

