// Reduce Function

// Case #1: Reduce function invoked on an empty array with and without initial value provided
// empty array
let studentAges = Array(5);

// sum function
const sum = function(accumulator, currentValue) {
	return accumulator + currentValue;
};

// 0
console.log(studentAges.reduce(sum, 0));
// typeError
console.log(studentAges.reduce(sum));

